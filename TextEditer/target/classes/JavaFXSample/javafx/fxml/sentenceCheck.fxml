<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox maxHeight="480.0" maxWidth="640.0" minHeight="480.0" minWidth="640.0" prefHeight="480.0" prefWidth="640.0" spacing="20.0" xmlns="http://javafx.com/javafx/13" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JavaFXSample.javafx.SentenceCheckController">
    <children>
        <Label fx:id="pathLabel" alignment="TOP_LEFT" prefWidth="640.0" text="ファイルパス表示" translateX="-50.0" translateY="-15.0">
         <graphic>
            <Button mnemonicParsing="false" onAction="#switchToEditFile" translateX="620.0">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@Icons/arrow-go-back-line.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </graphic></Label>
      <WebView fx:id="webView" prefHeight="415.0" prefWidth="600.0" translateY="-25.0" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
